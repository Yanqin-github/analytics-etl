# Development environment secrets template
# DO NOT STORE ACTUAL SECRETS IN THIS FILE
# Copy this file to secrets.yaml and fill in the values

# MongoDB
MONGODB_DEV_URI: mongodb+srv://username:password@host/database

# NewRelic
NEWRELIC_DEV_ACCOUNT_ID: "your-account-id"
NEWRELIC_DEV_API_KEY: "your-api-key"

# PostgreSQL
POSTGRES_DEV_HOST: "your-host"
POSTGRES_DEV_USER: "your-user"
POSTGRES_DEV_PASSWORD: "your-password"

# PowerBI
POWERBI_DEV_WORKSPACE_ID: "your-workspace-id"
POWERBI_DEV_DATASET_ID: "your-dataset-id"

# AWS (if needed)
AWS_DEV_ACCESS_KEY_ID: "your-access-key"
AWS_DEV_SECRET_ACCESS_KEY: "your-secret-key"
